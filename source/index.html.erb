    <span class="anchor" id="contact"></span>
    <section class="contact">
      <div class="container d-flex align-items-center justify-content-center flex-column">
        <h2 data-aos="fade-left" data-aos-once="true">Contact</h2>
        <div class="header-bar" data-aos="fade-left" data-aos-delay="200" data-aos-once="true"></div>
        <p class="contact-description" data-aos="fade-right" data-aos-delay="400" data-aos-once="true">Envie d'échanger sur ton projet ?</p>
        <div class="row">
          <div class="col d-flex justify-content-center">
            <form id="contactForm" data-aos="zoom-in" data-aos-delay="600" data-aos-duration="700" data-aos-once="true" method="POST" name="contactForm" class="contactForm">
              <div class="form-group">
                <input type="text"  name="name" placeholder="Nom*" required>
                <input type="email" name="email" placeholder="Email*" required>
                <input type="tel" name="téléphone" placeholder="Téléphone">
                <textarea name="message" placeholder="Message*" required></textarea>
              </div>
              <div id="success">
                <div>Votre message a été envoyé avec succès. Merci !
                  <span id="close" class="mdi"><i class="fas fa-times"></i></span>
                </div>
              </div>
              <input type="submit" value="Envoyer" id="submit">
            </form>
          </div>
        </div>
      </div>
